@using ExamTask.Models;
@model Setting
<div class="row">
    <div class="col-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Create Team</h4>
                <p class="card-description">
                    Create Setting
                </p>
                <form method="post" enctype="multipart/form-data" class="forms-sample">
                    <div class="form-group">
                        <label asp-for="Key">Key</label>
                        <input asp-for="Key" class="form-control" placeholder="Key">
                        <span asp-validation-for="Key" style="color:red"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Value">Value</label>
                        <input asp-for="Value" class="form-control" placeholder="Value">
                        <span asp-validation-for="Value" style="color:red"></span>
                    </div>


                    <button type="submit" class="btn btn-primary mr-2">Submit</button>
                    <a asp-controller="Team" asp-action="Index" class="btn btn-light">Go Back</a>
                </form>
            </div>
        </div>
    </div>
</div>


